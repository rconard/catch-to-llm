    "source-map": "^0.7.4",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.6.0",
